<?xml version="1.0" encoding="UTF-8"?><project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xmlns="http://maven.apache.org/POM/4.0.0"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">    <modelVersion>4.0.0</modelVersion>    <parent>        <groupId>com.alia</groupId>        <artifactId>platform</artifactId>        <version>1.0-SNAPSHOT</version>    </parent>    <groupId>com.alia</groupId>    <artifactId>platform-mock</artifactId>    <version>1.0-SNAPSHOT</version>    <packaging>jar</packaging>    <dependencies>        <dependency>            <groupId>commons-lang</groupId>            <artifactId>commons-lang</artifactId>            <version>2.5</version>        </dependency>    </dependencies>    <!-- Package as an executable jar -->    <build>        <pluginManagement>            <plugins>                <plugin>                    <groupId>org.apache.maven.plugins</groupId>                    <artifactId>maven-assembly-plugin</artifactId>                    <version>2.6</version>                    <configuration>                        <appendAssemblyId>false</appendAssemblyId>                        <descriptorRefs>                            <descriptorRef>jar-with-dependencies</descriptorRef>                        </descriptorRefs>                        <archive>                            <manifest>                                <mainClass>com.com.epcc.AliaDoReference</mainClass>                            </manifest>                        </archive>                    </configuration>                    <executions>                        <execution>                            <id>make-assembly</id>                            <phase>package</phase>                            <goals>                                <goal>assembly</goal>                            </goals>                        </execution>                    </executions>                </plugin>            </plugins>        </pluginManagement>    </build></project>